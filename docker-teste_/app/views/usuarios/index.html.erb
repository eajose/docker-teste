<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Rails</th>
      <th>Generate</th>
      <th>Scaffold</th>
      <th>User</th>
      <th>Nome completo</th>
      <th>Idade</th>
      <th>Cpf</th>
      <th>Data de nascimento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.rails %></td>
        <td><%= usuario.generate %></td>
        <td><%= usuario.scaffold %></td>
        <td><%= usuario.User %></td>
        <td><%= usuario.Nome_Completo %></td>
        <td><%= usuario.Idade %></td>
        <td><%= usuario.CPF %></td>
        <td><%= usuario.Data_de_Nascimento %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
